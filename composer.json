{
  "name": "motor-cms/motor-backend",
  "description": "Motor backend package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^7.2",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^6.0",
    "laravel/tinker": "^1.0",
    "motor-cms/motor-core": "0.9.*",
    "wpb/string-blade-compiler": "^4.0",
    "doctrine/dbal": "^2.6",
    "php-console/php-console": "^3.1",
    "guzzlehttp/guzzle": "^6.3",
    "barryvdh/laravel-ide-helper": "^2.6",
    "astrotomic/laravel-translatable": "^11.1",
    "laracasts/flash": "^3.0",
    "laravelcollective/html": "^6.0",
    "spatie/laravel-fractal": "^5.5",
    "intervention/imagecache": "^2.3",
    "league/glide-laravel": "^1.0",
    "lavary/laravel-menu": "^1.7",
    "nstapelbroek/culpa": "^4.0",
    "symfony/intl": "~4.2",
    "kris/laravel-form-builder": "^1.21",
    "spatie/laravel-medialibrary": "^7.12",
    "barryvdh/laravel-cors": "^0.11",
    "spatie/laravel-permission": "^3.0",
    "kalnoy/nestedset": "^5.0",
    "martinlindhe/laravel-vue-i18n-generator": "^0.1",
    "tightenco/ziggy": "^0.8",
    "laravel/horizon": "^3.3",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "beyondcode/laravel-dump-server": "^1.0",
    "symfony/css-selector": "~4.2",
    "symfony/dom-crawler": "~4.2",
    "orchestra/testbench": "~3.8",
    "laravel/dusk": "~5.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jmarcher/culpa-laravel-5"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dfox288/StringBladeCompiler"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Motor\\Backend\\": "src",
      "Tests\\Motor\\Backend\\": "tests"
    },
    "files": [
      "helpers/motor_backend_helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\Backend\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
