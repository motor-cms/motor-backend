    "spatie/laravel-fractal": "^5.2",
    "intervention/imagecache": "^2.3",
    "league/glide-laravel": "^1.0",
    "lavary/laravel-menu": "^1.6",
    "nstapelbroek/culpa": "^2.0",
    "symfony/intl": "^3.3",
